{
  "name": "@codai/gang-gpt-gta-v",
  "version": "1.0.0",
  "description": "AI-powered Grand Theft Auto V multiplayer server with intelligent NPCs, dynamic factions, and procedural missions",
  "main": "dist/server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dragoscv/gang-gpt-gta-v.git"
  },
  "homepage": "https://github.com/dragoscv/gang-gpt-gta-v#readme",
  "bugs": {
    "url": "https://github.com/dragoscv/gang-gpt-gta-v/issues"
  },
  "publishConfig": {
    "access": "public",
    "@codai:registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "dev": "tsx src/server.ts",
    "dev:api": "tsx watch src/server.ts",
    "dev:all": "pwsh -ExecutionPolicy Bypass -File scripts/start-dev-complete.ps1",
    "dev:full": "pwsh -ExecutionPolicy Bypass -File scripts/start-dev-complete.ps1",
    "dev:watch": "pwsh -ExecutionPolicy Bypass -File scripts/start-dev-watch.ps1",
    "dev:watch:backend": "pwsh -ExecutionPolicy Bypass -File scripts/start-service-watch.ps1 -Service backend",
    "dev:watch:frontend": "pwsh -ExecutionPolicy Bypass -File scripts/start-service-watch.ps1 -Service frontend",
    "dev:watch:ragemp": "pwsh -ExecutionPolicy Bypass -File scripts/start-service-watch.ps1 -Service ragemp",
    "dev:watch:all": "pwsh -ExecutionPolicy Bypass -File scripts/start-service-watch.ps1 -Service all",
    "test:watch-mode": "pwsh -ExecutionPolicy Bypass -File scripts/test-watch-mode.ps1",
    "test:watch-mode:backend": "pwsh -ExecutionPolicy Bypass -File scripts/test-watch-mode.ps1 -Service backend",
    "test:watch-mode:frontend": "pwsh -ExecutionPolicy Bypass -File scripts/test-watch-mode.ps1 -Service frontend",
    "test:watch-mode:ragemp": "pwsh -ExecutionPolicy Bypass -File scripts/test-watch-mode.ps1 -Service ragemp",
    "dev:status": "pwsh -ExecutionPolicy Bypass -File scripts/check-services.ps1",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "lint:check": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "docker:build": "docker build -t gang-gpt-server .",
    "docker:run": "docker run -p 4828:4828 gang-gpt-server",
    "clean": "pnpm exec rimraf dist node_modules/.cache",
    "fresh": "pnpm clean && pnpm install",
    "postinstall": "prisma generate",
    "clean:deps": "pnpm exec rimraf node_modules pnpm-lock.yaml && pnpm install",
    "clean:build": "pnpm exec rimraf dist coverage .next",
    "clean:all": "pnpm run clean:build && pnpm run clean:deps",
    "verify:ragemp": "node verify-ragemp-integration.js"
  },
  "keywords": [
    "gta-v",
    "rage-mp",
    "ai",
    "openai",
    "roleplay",
    "multiplayer",
    "gaming",
    "typescript",
    "nodejs"
  ],
  "author": "GangGPT Development Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@trpc/server": "^11.2.0",
    "@types/compression": "^1.8.1",
    "@types/validator": "^13.15.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "isomorphic-dompurify": "^2.25.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.3",
    "openai": "^5.0.1",
    "prom-client": "^15.1.3",
    "socket.io": "^4.7.4",
    "superjson": "^2.2.2",
    "validator": "^13.15.15",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^4.28.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.9.4",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitest/coverage-v8": "3.2.2",
    "@vitest/ui": "^3.2.2",
    "axios": "^1.9.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "lint-staged": "^15.2.0",
    "node-fetch": "^2.7.0",
    "playwright": "^1.40.1",
    "prettier": "^3.1.0",
    "prisma": "^5.7.1",
    "redis-server": "^1.2.2",
    "supertest": "^7.1.1",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2",
    "vitest": "^3.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}