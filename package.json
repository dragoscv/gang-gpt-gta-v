{
  "name": "@ganggpt/monorepo",
  "version": "2.0.0",
  "description": "Next-generation AI-powered Grand Theft Auto V multiplayer server - Advanced monorepo architecture",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/dragoscv/gang-gpt-gta-v.git"
  },
  "homepage": "https://github.com/dragoscv/gang-gpt-gta-v#readme",
  "bugs": {
    "url": "https://github.com/dragoscv/gang-gpt-gta-v/issues"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "!packages/blockchain"
  ],
  "scripts": {
    "start": "concurrently \"./redis-windows/redis-server.exe ./redis-windows/ganggpt-redis.conf\" \"cd apps/backend && pnpm dev\" \"cd apps/web && pnpm dev\" --names \"redis,backend,web\" --prefix-colors \"red,blue,green\"",
    "start:game": "concurrently \"pnpm dev:backend\" \"pnpm dev:web\" \"pwsh scripts/dev.ps1 -Mode start -Service redis\"",
    "build": "turbo build --filter=!@ganggpt/blockchain",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "test": "turbo test --filter=!@ganggpt/blockchain",
    "test:watch": "turbo test --watch",
    "test:coverage": "turbo test:coverage",
    "test:e2e": "turbo test:e2e",
    "format": "turbo format",
    "typecheck": "turbo typecheck",
    "clean": "turbo clean && rimraf node_modules/.turbo",
    "reset": "pnpm clean && rimraf node_modules pnpm-lock.yaml && pnpm install",
    "db:generate": "turbo db:generate",
    "db:push": "turbo db:push",
    "db:migrate": "turbo db:migrate",
    "db:studio": "turbo db:studio",
    "dev:backend": "turbo dev --filter=@ganggpt/backend",
    "dev:web": "turbo dev --filter=@ganggpt/web",
    "build:backend": "turbo build --filter=@ganggpt/backend",
    "build:web": "turbo build --filter=@ganggpt/web",
    "build:all": "turbo build",
    "docker:build": "docker build -t ganggpt-server .",
    "docker:run": "docker run -p 4828:4828 ganggpt-server",
    "fresh": "pnpm clean && pnpm install",
    "clean:deps": "pnpm exec rimraf node_modules pnpm-lock.yaml && pnpm install",
    "clean:build": "pnpm exec rimraf dist coverage .next",
    "clean:all": "pnpm run clean:build && pnpm run clean:deps",
    "verify:ragemp": "node verify-ragemp-integration.js"
  },
  "keywords": [
    "gta-v",
    "rage-mp",
    "ai",
    "openai",
    "roleplay",
    "multiplayer",
    "gaming",
    "typescript",
    "nodejs"
  ],
  "author": "GangGPT Development Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@trpc/server": "^11.4.3",
    "@types/compression": "^1.8.1",
    "@types/validator": "^13.15.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "isomorphic-dompurify": "^2.25.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.3",
    "openai": "^5.0.1",
    "prom-client": "^15.1.3",
    "socket.io": "^4.7.4",
    "superjson": "^2.2.2",
    "validator": "^13.15.15",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^4.28.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.9.4",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitest/coverage-v8": "3.2.2",
    "@vitest/ui": "^3.2.2",
    "concurrently": "^9.2.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "lint-staged": "^15.2.0",
    "playwright": "^1.40.1",
    "prettier": "^3.1.0",
    "prisma": "^5.7.1",
    "supertest": "^7.1.1",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.6.0",
    "turbo": "^2.5.4",
    "typescript": "^5.3.2",
    "vitest": "^3.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}